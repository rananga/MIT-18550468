	</connectionStrings>
	<appSettings>
		<add key="webpages:Version" value="3.0.0.0" />
		<add key="webpages:Enabled" value="false" />
		<add key="ClientValidationEnabled" value="true" />
		<add key="UnobtrusiveJavaScriptEnabled" value="true" />
		<add key="StudentImagePath" value="\Content\Images\Student" />
		<add key="StaffImagePath" value="\Content\Images\Staff" />
		<add key="ParentImagePath" value="\Content\Images\Parent" />
		<add key="VisitorImagePath" value="\Content\Images\Visitor" />
