<div id=@ViewBag.DialogID class="columns" data-url=@ViewBag.DataUrl>

    <div class="ui-columns-search">
        <input name="query" class="ui-table-search NoClearIcon" type="search" placeholder="Search">
    </div>

    <div class="ui-columns-table" style="display: none; overflow: auto;">

    </div>

    <div class="ui-columns-progress">
        <div class="ui-table-progress" style="background: rgb(128, 128, 128); height: 400px; text-align: center; vertical-align: middle; display: table-cell;">
            <div class="ui-table-progress-img" style="background: rgb(255, 255, 255); padding: 10px; border: 1px solid rgb(0, 0, 0); border-image: none; display: inline-block;">
                <span>Loading...</span>
                <br>
                <img alt="" src="~/Content/Images/Processing.gif">
            </div>
        </div>
    </div>

    <div class="ui-columns-footer">
        <div class="ui-table-footer">
            <div class="ui-table-results">
                <div>Results: <strong>0 – 0</strong> of <strong>0</strong></div>
                <div style="margin: 5px 0px;">
                    Page <strong>0</strong> of <strong>0</strong>
                </div>
            </div>
            <div class="ui-table-controls">
                <div class="ui-table-show-rows">
                    <span>
                        Show rows:&nbsp;
                        <select>
                            <option selected="selected" value="5">5</option>
                            <option value="10">10</option>
                        </select>
                    </span>
                </div>
                <div>
                    <div class="ui-table-control-disabled" data-columns-page="0">
                        <div class="ui-table-control-first-img"></div>
                    </div>
                    <div class="ui-table-control-disabled" data-columns-page="0">
                        <div class="ui-table-control-prev-img"></div>
                    </div>
                    <span>
                        <input name="page" class="ui-table-control-gotopage" style="width: 8px;" type="text">
                    </span>
                    <div class="ui-table-control-disabled" data-columns-page="0">
                        <div class="ui-table-control-next-img"></div>
                    </div>
                    <div class="ui-table-control-disabled" data-columns-page="0">
                        <div class="ui-table-control-last-img"></div>
                    </div>
                </div>
            </div><div style="clear: both;"></div>
        </div>
    </div>

    <div style="padding-top:10px;text-align:right;border-top: 1px solid #e5e5e5;">
        <input type="button" class="btn btn-default ui-columns-foot-clear" style="min-width:75px" value="Clear" />
        <input type="button" class="btn btn-primary ui-columns-foot-ok" style="min-width:75px" value="Ok" />
        <input type="button" class="btn btn-default ui-columns-foot-cancel" style="min-width:75px" value="Cancel" />
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () { initPopUpSel('@ViewBag.DialogID'); });
</script>
